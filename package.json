{
	"name": "echati18n",
	"displayName": "EchatI18n",
	"description": "The echat i18n tool for internal developer",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.echatLan"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.echatLan",
				"title": "Hello Echat Lan"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
